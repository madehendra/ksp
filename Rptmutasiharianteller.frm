Attribute VB_Name = "RptMutasiHarianTeller1"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Dim dbData As New ADODB.Recordset
Dim objData As New CodeSuiteLibrary.data
Dim vaArray As New XArrayDB
Dim lPreview As Boolean

Private Sub initvalue()
  dTgl(0).Value = Date
  dTgl(1).Value = Date
End Sub

Private Sub cmdKeluar_Click()
  Unload Me
End Sub

Private Sub cmdPreview_Click()
  lPreview = True
  GetSQL
End Sub

Private Sub cmdRefresh_Click()
  lPreview = False
  GetSQL
End Sub

Private Sub Form_Load()
Dim n As Single

  CenterForm Me, True
  initvalue
  
  TabIndex dTgl(0), n
  TabIndex dTgl(1), n
  TabIndex cmdRefresh, n
  TabIndex cmdPreview, n
  TabIndex cmdKeluar, n
End Sub

Private Sub GetSQL()
Dim n As Long
Dim nSaldo As Double
Dim cSQL As String
Dim nDebet As Double
Dim nKredit As Double

  nDebet = 0
  nKredit = 0
  vaArray.ReDim 0, -1, 0, 5
  Set dbData = objData.Browse(GetDSN, "BukuBesar", "Faktur,Keterangan,Debet,Kredit", "Tgl", sisGTEqual, Format(dTgl(0).Value, "yyyy-mm-dd"), " And Tgl <= '" & Format(dTgl(1).Value, "yyyy-MM-dd") & "' and Rekening = '" & cKasTeller & "'", "datetime,Faktur")
  If Not dbData.eof Then
    dbData.MoveFirst
    Do While Not dbData.eof
      vaArray.InsertRows vaArray.UpperBound(1) + 1
      n = vaArray.UpperBound(1)
      vaArray(n, 0) = n + 1
      vaArray(n, 1) = (dbData!Faktur)
      vaArray(n, 2) = (dbData!Keterangan)
      vaArray(n, 3) = (dbData!Debet)
      vaArray(n, 4) = (dbData!Kredit)
      vaArray(n, 5) = vaArray(IIf(n > 0, n - 1, n), 5) + vaArray(n, 3) - vaArray(n, 4)
      
      nDebet = nDebet + vaArray(n, 3)
      nKredit = nKredit + vaArray(n, 4)
      dbData.MoveNext
    Loop
  End If
    
  TDBGrid1.Columns(3).FooterText = Format(nDebet, "###,###,###,###,##0.00")
  TDBGrid1.Columns(4).FooterText = Format(nKredit, "###,###,###,###,##0.00")
  Set TDBGrid1.Array = vaArray
  TDBGrid1.ReBind
  
  If lPreview = True Then
    rpt
  End If
End Sub

Private Sub TDBGrid1_FormatText(ByVal ColIndex As Integer, Value As Variant, Bookmark As Variant)
  If Value = 0 Then
    Value = ""
  Else
    Value = Format(Value, "###,###,###,###,##0.00")
  End If
End Sub

